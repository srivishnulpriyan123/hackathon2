void merge(int* nums1, int nums1Size, int m, int* nums2, int nums2Size, int n) 
{
    int j=0;
    for(int i=m;i<m+n;i++)
    {
        nums1[i]=nums2[j];
        j++;
    }
    for(int i=0;i<n+m-1;i++)
    {
        for(int j=i+1;j<n+m;j++)
        {
            if(nums1[i]>nums1[j])
            {
                int t=nums1[i];
                nums1[i]=nums1[j];
                nums1[j]=t;
            }
        }
    }
    for(int i=0;i<m+n;i++)
    {
        printf("%d",nums1[i]);
    }
}
